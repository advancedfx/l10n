msgctxt "ApplicationName"
msgid "HLAE"
msgstr "HLAE"

msgctxt "HlaeLocalesTitle"
msgid "Translations"
msgstr "Tłumaczenia"

msgctxt "Manufacturer"
msgid "advancedfx.org"
msgstr "advancedfx.org"

msgctxt "DowngradeErrorMessage"
msgid "A newer version of [ProductName] is already installed."
msgstr "Posiadasz najnowszą wersję [ProductName]"

msgctxt "FfmpegTitle"
msgid "FFMPEG"
msgstr "FFMPEG"

msgctxt "FfmpegDescription"
msgid "FFMPEG can be used  for encoding mirv_streams to videos."
msgstr "To modify an existing HLAE installation, please use the \"Modify\" option in the \"Application and Windows\" panel."

msgctxt "FfmpegDlgTitle"
msgid "{\\WixUI_Font_Title}FFMPEG"
msgstr "{\\WixUI_Font_Title}FFMPEG"

msgctxt "FfmpegDlgDescription"
msgid "Install FFMPEG or use exsiting one."
msgstr "Zainstaluj FFMPEG albo użyj jego istniejącej wersji."

msgctxt "FfmpegDlgReinstall"
msgid "Reinstall FFMPEG"
msgstr "Przeinstaluj FFMPEG"

msgctxt "FfmpegDlgDownloaded"
msgid "FFMPEG will be downloaded from https://ffmpeg.zeranoe.com/builds/ and installed using your current internet connection and proxy settings."
msgstr "FFMPEG zostanie pobrany z https://ffmpeg.zeranoe.com/builds/ oraz zainstalowany przy użyciu obecnego połączenia internetowego oraz ustawień proxy."

msgctxt "FfmpegDlgVersion"
msgid "Select version:"
msgstr "Wybierz wersję:"

msgctxt "FfmpegDlgLatest"
msgid "latest (recommended)"
msgstr "najnowsza (zalecana)"

msgctxt "FfmpegDlgCustom"
msgid "Select existing on disk"
msgstr "Wybierz istniejącą na dysku"

msgctxt "FfmpegDlgLinking"
msgid "Select linking:"
msgstr "Wybierz linking:"

msgctxt "FfmpegDlgStatic"
msgid "static (faster, 100 MB download, recommended)"
msgstr "statyczne (szybsze, 100 MB do pobrania, zalecane)"

msgctxt "FfmpegDlgShared"
msgid "shared (smaller, 50 MB download)"
msgstr "dzielone (mniejsze, 50 MB do pobrania)"

msgctxt "FfmpegDlgLicense"
msgid "Select license:"
msgstr "Wybierz licencję:"

msgctxt "FfmpegDlgGpl3"
msgid "GPL3 (complete, recommended)"
msgstr "GPL3 (pełne, zalecane)"

msgctxt "FfmpegDlgLgpl3"
msgid "LGPL3 (less features, more permissive)"
msgstr "LGPL3 (mniej funkcji, bardziej przywalające)"

msgctxt "FfmpegDlgPath"
msgid "Please enter the full path to your custom ffmpeg.exe here:"
msgstr "Proszę wskazać pełną ścieżkę do niestandardowego ffmpeg.exe :"

msgctxt "FfmpegDlgPathBrowse"
msgid "Browse"
msgstr "Przeglądaj"

msgctxt "FfmpegDlgPathError"
msgid "{\\WixUI_Font_Bold}File does not exist or is not accessible. Please fix and click Next button to try again."
msgstr "{\\WixUI_Font_Bold}Plik nie istnieje, bądź jest niedostępny. Proszę naprawić i kliknąć \"Next\", aby ponownie spróbować."

msgctxt "InstallFfmpeg"
msgid "Installing FFMPEG ..."
msgstr "Instaluję FFMPEG..."

msgctxt "InstallFfmpegConnect"
