msgctxt "ApplicationName"
msgid "HLAE"
msgstr "HLAE"

msgctxt "HlaeLocalesTitle"
msgid "Translations"
msgstr "Переводы"

msgctxt "Manufacturer"
msgid "advancedfx.org"
msgstr "advancedfx.org"

msgctxt "DowngradeErrorMessage"
msgid "A newer version of [ProductName] is already installed."
msgstr "Более новая версия [ProductName] уже установлена."

msgctxt "FfmpegTitle"
msgid "FFMPEG"
msgstr "FFMPEG"

msgctxt "FfmpegDescription"
msgid "FFMPEG can be used  for encoding mirv_streams to videos."
msgstr "FFMPEG может быть использован  для перекодировки mirv_streams в видео."

msgctxt "FfmpegDlgTitle"
msgid "{\\WixUI_Font_Title}FFMPEG"
msgstr "{\\WixUI_Font_Title}FFMPEG"

msgctxt "FfmpegDlgDescription"
msgid "Install FFMPEG or use exsiting one."
msgstr "Установить FFMPEG или используйте имеющийся."

msgctxt "FfmpegDlgReinstall"
msgid "Reinstall FFMPEG"
msgstr "Установка FFMPEG"

msgctxt "FfmpegDlgDownloaded"
msgid "FFMPEG will be downloaded from https://ffmpeg.zeranoe.com/builds/ and installed using your current internet connection and proxy settings."
msgstr "FFMPEG можно скачать с https://ffmpeg.zeranoe.com/builds/ и установить, используя ваши настройки интернет соединения и прокси."

msgctxt "FfmpegDlgVersion"
msgid "Select version:"
msgstr "Выберите версию:"

msgctxt "FfmpegDlgLatest"
msgid "latest (recommended)"
msgstr "последняя (рекомендуется)"

msgctxt "FfmpegDlgCustom"
msgid "Select existing on disk"
msgstr "Уже установлен (выбрать на диске)"

msgctxt "FfmpegDlgLinking"
msgid "Select linking:"
msgstr "Выбрать сборку:"

msgctxt "FfmpegDlgStatic"
msgid "static (faster, 100 MB download, recommended)"
msgstr "статичная (быстрее, 100МБ загрузки, рекомендуется)"

msgctxt "FfmpegDlgShared"
msgid "shared (smaller, 50 MB download)"
msgstr "общая (меньше, 50МБ загрузки)"

msgctxt "FfmpegDlgLicense"
msgid "Select license:"
msgstr "Выбрать лицензию:"

msgctxt "FfmpegDlgGpl3"
msgid "GPL3 (complete, recommended)"
msgstr "GPL3 (полная, рекомендуется)"

msgctxt "FfmpegDlgLgpl3"
msgid "LGPL3 (less features, more permissive)"
msgstr "LGPL3 (меньше возможностей, больше разрешений)"

msgctxt "FfmpegDlgPath"
msgid "Please enter the full path to your custom ffmpeg.exe here:"
msgstr "Пожалуйста введите полный путь до вашего пользовательского ffmpeg.exe здесь:"

msgctxt "FfmpegDlgPathBrowse"
msgid "Browse"
msgstr "Обзор"

msgctxt "FfmpegDlgPathError"
msgid "{\\WixUI_Font_Bold}File does not exist or is not accessible. Please fix and click Next button to try again."
msgstr "{\\WixUI_Font_Bold}Файл не существует или недоступен. Пожалуйста, исправьте ошибку и нажмите Далее."

msgctxt "InstallFfmpeg"
msgid "Installing FFMPEG ..."
msgstr "Установка FFMPEG ..."

msgctxt "InstallFfmpegConnect"
msgid "Connecting to [AFX_FFMPEGURL]"
msgstr "Соединяемся с [AFX_FFMPEGURL]"

msgctxt "InstallFfmpegConnect_Template"
msgid "..."
msgstr "..."

msgctxt "InstallFfmpegDownload"
msgid "Downloading [AFX_FFMPEGURL]"
msgstr "Скачиваем [AFX_FFMPEGURL]"

msgctxt "InstallFfmpegDownload_Template"
msgid "[1] of [2] Bytes"
msgstr "[1] из [2] Байт"

msgctxt "InstallFfmpegExtract"
msgid "Extracting FFMPEG"
msgstr "Распаковка FFMPEG"

msgctxt "InstallFfmpegExtract_Template"
msgid "Item [1] of [2] ..."
msgstr "Элемент [1] из [2] ..."

msgctxt "AppFolderMustBeLatin"
msgid "The HLAE application folder path contains invalid characters: Only basic latin character set is allowed (7bit ASCII)."
msgstr "Папка установки HLAE содержит недопустимые символы: Разрешены только основные латинские символы (7bit ASCII)."

msgctxt "RemoveHlaeAppDataFolder"
msgid "Remove HLAE application data folder (not recommended)."
msgstr "Удалить папку HLAE из AppData (не рекомендуется)"

